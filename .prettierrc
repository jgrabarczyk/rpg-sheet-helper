{
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "objectWrap": "preserve",
  "bracketSameLine": false,
  "endOfLine": "crlf",
  "singleAttributePerLine": true,
  "overrides": [
    {
      "files": "**/*.html",
      "options": {
        "parser": "angular"
      }
    }
  ]
}
